'use strict';

ms2Extend.combo.select = function (config) {
    config = config || {};
    Ext.applyIf(config, {
        store: [
            [0, _('no')],
            [1, _('yes')],
        ],
        fields: ['value', 'title'],
        displayField: 'title',
        valueField: 'value'
    });
    ms2Extend.combo.select.superclass.constructor.call(this, config);
};
Ext.extend(ms2Extend.combo.select, ms2Extend.combo.selectLocal);
Ext.reg('ms2extend-combo-select', ms2Extend.combo.select);
